<h1 class='title'>{{name}}</h1>
<div class='container'>
  <div class='info'>
    <p><b>Capital:</b>{{capital}}</p>
    <p><b>Population:</b>{{population}}</p>
    <p><b>Languages:</b></p>
    <ul>
      {{#each languages}}
        <li class='item'>{{this.name}}</li>
      {{/each}}
    </ul>
  </div>
  <div class='flag'>
    <img src='{{flag}}' width='400' height='300' alt='flag' />
  </div>
</div>